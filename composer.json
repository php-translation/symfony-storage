{
    "name": "php-translation/symfony-storage",
    "description": "A translation file storage using Symfony translation component.",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "php-translation/common": "^0.2.1",
        "symfony/translation": "^2.7 || ^3.0",
        "nyholm/nsa": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5 || ^6.2",
        "symfony/framework-bundle": "^2.7 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "Translation\\SymfonyStorage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Translation\\SymfonyStorage\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
